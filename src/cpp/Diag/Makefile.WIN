##############################################################################
## Local module list
MAKEXE :=
MAKEXE += Test_Mem.exe

##############################################################################
## Define default action
ifeq "" "$(DEFAULT)"
DEFAULT:= $(MAKEXE)
endif

default: $(DEFAULT)

##############################################################################
## TARGET: Test_Mem
Test_Mem.exe: $(MAKOBJ)
	$(LD) /out:"$@" $(CLOAD) $^ $(CLIBS)

